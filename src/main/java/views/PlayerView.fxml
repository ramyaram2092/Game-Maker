<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>

<AnchorPane prefHeight="900.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Line endX="500.0" layoutX="101.0" layoutY="76.0" startX="-100.0" />
      <Button fx:id="playStopButton" layoutX="10.0" layoutY="14.0" mnemonicParsing="false" onAction="#playStopButtonClicked" prefHeight="47.0" prefWidth="108.0" text="Play" />
      <Button fx:id="undoPauseButton" layoutX="130.0" layoutY="14.0" mnemonicParsing="false" onAction="#undoPauseButtonClicked" prefHeight="47.0" prefWidth="108.0" text="Undo" />
      <Button fx:id="saveButton" layoutX="250.0" layoutY="14.0" mnemonicParsing="false" onAction="#saveButtonClicked" prefHeight="47.0" prefWidth="108.0" text="Save" />
      <Button fx:id="loadButton" layoutX="370.0" layoutY="14.0" mnemonicParsing="false" onAction="#loadButtonClicked" prefHeight="47.0" prefWidth="108.0" text="Load" />
      <Canvas fx:id="gameCanvas" height="826.0" layoutX="1.0" layoutY="76.0" onMouseClicked="#canvasClicked" onMouseDragged="#canvasDragged" onMouseReleased="#canvasReleased" width="600.0" />
   </children>
</AnchorPane>
